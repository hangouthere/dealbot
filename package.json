{
  "name": "dealbot",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "./dist/app.js",
  "main": "./dist/app.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "docker-compose": "npm i --force && echo \"Running in docker-compose... You should run interactively by attaching:\n\t\tdocker exec -it -u abc `hostname` npm run dev\" && tail -f /dev/null",
    "prestart": "mkdir -p dist && knex migrate:latest",
    "build": "parcel build ./src/app.js",
    "start": "npm run build && npx -y .",
    "dev": "NODE_ENV=development concurrently -r 'npm:dev-concurrently*'",
    "dev-concurrently:watch": "parcel watch ./src/app.js",
    "predev-concurrently:run": "npm run prestart",
    "dev-concurrently:run": "nodemon --inspect=0.0.0.0:9229 --trace-warnings -L . -- ${APP_FLAGS}"
  },
  "dependencies": {
    "bookshelf": "^1.2.0",
    "chalk": "^4.1.2",
    "elapsed-time": "0.0.1",
    "import-jsx": "^4.0.0",
    "knex": "^0.95.14",
    "limiter": "^2.1.0",
    "loglevel": "^1.7.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "md5": "^2.3.0",
    "meow": "^9.0.0",
    "mustache": "^4.2.0",
    "node-fetch": "^2.6.6",
    "react": "^17.0.2",
    "rss-parser": "^3.12.0",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@types/react": "^17.0.34",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.0.1"
  },
  "alias": {
    "_Shared": "./src/_shared",
    "-": "./src"
  }
}
